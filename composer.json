{
    "name": "fmtod/laravel-money",
    "description": "Laravel Money",
    "keywords": [
        "laravel",
        "money",
        "currency"
    ],
    "homepage": "https://github.com/FmTod/laravel-money-source",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "require": {
        "php": "^8.0|^8.2",
        "ext-intl": "*",
        "ext-json": "*",
        "illuminate/support": "^9.49",
        "illuminate/view": "^9.49",
        "moneyphp/money": "^4.1",
        "spatie/laravel-package-tools": "^1.14"
    },
    "require-dev": {
      "laravel/pint": "^1.0",
      "nunomaduro/collision": "^6.0",
      "nunomaduro/larastan": "^2.0.1",
      "orchestra/testbench": "^7.0",
      "pestphp/pest": "^1.21",
      "pestphp/pest-plugin-laravel": "^1.1",
      "phpunit/phpunit": "^9.5",
      "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "FmTod\\Money\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FmTod\\Money\\Tests\\": "tests/"
        },
        "files": [
            "tests/Database/Migrations/2020_04_30_000000_create_users_table.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "FmTod\\Money\\MoneyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest",
        "lint": "vendor/bin/pint"
    }
}
