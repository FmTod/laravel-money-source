{
    "name": "fmtod/laravel-money",
    "description": "Laravel Money",
    "keywords": [
        "laravel",
        "money",
        "currency"
    ],
    "homepage": "https://github.com/FmTod/laravel-money-source",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-intl": "*",
        "ext-json": "*",
        "brick/money": "^0.5.1",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0",
        "moneyphp/money": "^3.3"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.6",
        "illuminate/filesystem": "^7.0|^8.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^8.5|^9.0",
        "spatie/phpunit-watcher": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "FmTod\\Money\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FmTod\\Money\\Tests\\": "tests/"
        },
        "files": [
            "tests/Database/Migrations/2020_04_30_000000_create_users_table.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "FmTod\\Money\\Providers\\MoneyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "watch": "vendor/bin/phpunit-watcher watch"
    }
}
